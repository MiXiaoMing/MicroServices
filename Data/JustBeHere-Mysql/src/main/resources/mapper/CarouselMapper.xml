<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.microservices.data.justbehere.mysql.CarouselMapper">

    <sql id="column">
	  <![CDATA[
		  id, carousel_code as carouselCode, classify, code, path, delflag, update_time as updateTime, create_time as createTime
        ]]>
    </sql>


    <insert id="insert" parameterType="com.microservices.common.feignclient.data.justbehere.result.Carousel">
        insert into t_carousel
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                id,
            </if>
            <if test="carouselCode != null">
                carousel_code,
            </if>
            <if test="classify != null">
                classify,
            </if>
            <if test="code != null">
                code,
            </if>
            <if test="path != null">
                path,
            </if>
            <if test="delflag != null">
                delflag,
            </if>
            <if test="updateTime != null">
                update_time,
            </if>
            <if test="createTime != null">
                create_time,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">
                #{id,jdbcType=VARCHAR},
            </if>
            <if test="carouselCode != null">
                #{carouselCode,jdbcType=VARCHAR},
            </if>
            <if test="code != null">
                #{code,jdbcType=VARCHAR},
            </if>
            <if test="classify != null">
                #{classify,jdbcType=VARCHAR},
            </if>
            <if test="path != null">
                #{path,jdbcType=VARCHAR},
            </if>
            <if test="delflag != null">
                #{delflag,jdbcType=VARCHAR},
            </if>
            <if test="updateTime != null">
                #{updateTime,jdbcType=TIMESTAMP},
            </if>
            <if test="createTime != null">
                #{createTime,jdbcType=TIMESTAMP},
            </if>
        </trim>
    </insert>

    <select id="selectList" parameterType="java.util.Map" resultType="com.microservices.common.feignclient.data.justbehere.result.Carousel">
        select
        <include refid="column"/>
          from t_carousel
          where
          <![CDATA[
            delflag <> 'D'
          ]]>
          <if test="carouselCode != null">
            and carousel_code = #{carouselCode,jdbcType=VARCHAR}
          </if>
          <if test="classify != null">
            and classify = #{classify,jdbcType=VARCHAR}
          </if>
          <if test="code != null">
            and code = #{code,jdbcType=VARCHAR}
          </if>
    </select>

</mapper>